<?xml version="1.0" ?>
<robot name="dobot">
    <!-- Defining the colors used in this robot -->
    <material name="Black">
        <color rgba="0 0 0 1" />
    </material>
    <material name="White">
        <color rgba="1 1 1 1" />
    </material>
    <material name="Grey">
        <color rgba="0.5 0.5 0.5 1" />
    </material>
    <link name="world" />
    <joint name="joint1_world" type="fixed">
        <parent link="world" />
        <child link="base_link" />
    </joint>
    <link name="base_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 3.14159265" />
            <geometry>
                <mesh filename="package://armbot_description/meshes/base_link.STL" />
            </geometry>
            <material name="White" />
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://armbot_description/meshes/base_link.STL" />
            </geometry>
        </collision>
        
<inertial>
            <mass value="0.1" />
            <inertia ixx="0.03" iyy="0.03" izz="0.03" ixy="0.0" ixz="0.0" iyz="0.0" />
        </inertial>

    </link>
    <joint name="joint_1" type="revolute">
        <origin xyz="0 0 0.024" rpy="0 0 -1.570796325" />
        <parent link="base_link" />
        <child link="link_1" />
        <axis xyz="0 0 1" />
        <limit effort="150.0" lower="-2.25" upper="2.25" velocity="3.15" />
        <!--  от -128,916° до 128,916° -->
    </joint>
    <link name="link_1">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://armbot_description/meshes/link_1.dae" />
            </geometry>
            <!--material name="White"/-->
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://armbot_description/meshes/link_1.STL" />
            </geometry>
        </collision>
        
<inertial>
            <mass value="0.1" />
            <inertia ixx="0.03" iyy="0.03" izz="0.03" ixy="0.0" ixz="0.0" iyz="0.0" />
        </inertial>

    </link>
    <joint name="joint_2" type="revolute">
        <origin xyz="-0.01175 0 0.114" rpy="1.570796325 0 -1.570796325" />
        <parent link="link_1" />
        <child link="link_2" />
        <axis xyz="0 0 1" />
        <limit effort="150.0" lower="0" upper="1.570796325" velocity="3.15" />
        <!--  от 0° до 90° -->
    </joint>
    <link name="link_2">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://armbot_description/meshes/link_2.dae" />
            </geometry>
            <!--material name="Black"/-->
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://armbot_description/meshes/link_2.STL" />
            </geometry>
        </collision>
        
<inertial>
            <mass value="0.1" />
            <inertia ixx="0.03" iyy="0.03" izz="0.03" ixy="0.0" ixz="0.0" iyz="0.0" />
        </inertial>

    </link>
    <joint name="joint_3" type="revolute">
        <origin xyz="0.02699 0.13228 -0.01175" rpy="0 3.14159265 0" />
        <parent link="link_2" />
        <child link="link_3" />
        <axis xyz="0 0 1" />
        <limit effort="150.0" lower="0" upper="1.570796325" velocity="3.15" />
        <!--  от 0° до 90° -->
    </joint>
    <link name="link_3">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://armbot_description/meshes/link_3.dae" />
            </geometry>
            <!-- material name="White"/-->
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://armbot_description/meshes/link_3.dae" />
            </geometry>
        </collision>
        
<inertial>
            <mass value="0.1" />
            <inertia ixx="0.03" iyy="0.03" izz="0.03" ixy="0.0" ixz="0.0" iyz="0.0" />
        </inertial>

    </link>
    <joint name="joint_4" type="revolute">
        <origin xyz="0.07431 -0.12684 0.0" rpy="0 3.14159265 0" />
        <parent link="link_3" />
        <child link="link_4" />
        <axis xyz="0 0 1" />
        <limit effort="150.0" lower="-3.14159265" upper="3.14159265" velocity="3.15" />
