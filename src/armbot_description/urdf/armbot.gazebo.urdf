        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://armbot_description/meshes/link_4.STL" />
            </geometry>
        </collision>
        
<inertial>
            <mass value="0.1" />
            <inertia ixx="0.03" iyy="0.03" izz="0.03" ixy="0.0" ixz="0.0" iyz="0.0" />
        </inertial>

    </link>
    <joint name="joint_end" type="fixed">
        <origin xyz="-0.06 0.0 0.0" rpy="1.570796325 0 0" />
        <parent link="link_4" />
        <child link="link_end" />
        <axis xyz="0 0 1" />
        <limit effort="150.0" lower="-3.14159265" upper="3.14159265" velocity="3.15" />
        <!-- от -180° до 180° -->
    </joint>
    <link name="link_end">
        <visual>
            <origin xyz="-0.01 0 -0.045" rpy="1.570796325 0 0" />
            <geometry>
                <mesh filename="package://armbot_description/meshes/link_end.STL" />
            </geometry>
            <material name="Black" />
        </visual>
        <collision>
            <origin xyz="-0.01 0 -0.045" rpy="1.570796325 0 0" />
            <geometry>
                <mesh filename="package://armbot_description/meshes/link_end.STL" />
            </geometry>
        </collision>
        
<inertial>
            <mass value="0.1" />
            <inertia ixx="0.03" iyy="0.03" izz="0.03" ixy="0.0" ixz="0.0" iyz="0.0" />
        </inertial>

    </link>
    <joint name="joint_grip" type="revolute">
