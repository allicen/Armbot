<launch>
  <arg
    name="dobot" />
  <arg
    name="gui"
    default="True" />
  <param
    name="robot_description"
    textfile="$(find armbot_description)/urdf/dobot.urdf" />
  <param
    name="use_gui"
    value="$(arg gui)" />

  <node
    name="rviz"
    pkg="rviz"
    type="rviz"
    args="-d $(find armbot_description)/config/armbot.rviz"/>

  <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher"/>

  <node
    name="robot_state_publisher"
    pkg="robot_state_publisher"
    type="state_publisher" />

  <include file="$(find keyboard_description)/launch/keyboard.launch" />


</launch>
